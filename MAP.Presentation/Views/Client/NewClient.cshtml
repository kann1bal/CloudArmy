        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->

            <section class="content">
                <div class="row">
                    <div class="col-md-3">
                        <a href="~/Client/Create" class="btn btn-primary btn-block margin-bottom"> Add Client </a>

                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">Type Filter</h3>

                                <div class="box-tools">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body no-padding">
                                <ul class="nav nav-pills nav-stacked">

                                    <li>
                                        @Html.ActionLink("Actual Client", "ActualClient")
                                    </li>
                                    <li>
                                        @Html.ActionLink("New Client", "NewClient")
                                    </li>
                                    <li>
                                        @Html.ActionLink("Old Client", "OldClient")
                                    </li>
                                </ul>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /. box -->
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">Category Filter</h3>

                                <div class="box-tools">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body no-padding">
                                <ul class="nav nav-pills nav-stacked">

                                    <li>
                                        @Html.ActionLink("Public Client", "PublicClient")
                                    </li>
                                    <li>
                                        @Html.ActionLink("Private Client", "PrivateClient")
                                    </li>
                                </ul>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                    <!-- /.col -->

                    <div class="col-md-9">
                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title"></h3>
                                
                                    @Html.ActionLink("Refresh","Index")
                                
                                <div class="box-tools">
                                    @using (Html.BeginForm())
                                    { <table class="input-group input-group-sm" style="width: 150px;">
                                        <tr>
                                            <td>
                                                <div>
                                                    @Html.TextBox("searchString")
                                                </div>
                                            </td>
                                            <td>
                                                <div>
                                                    @*  <button type="submit" id="submit" class="btn btn-default"><i class="fa fa-search"></i></button>*@
                                                    <input type="submit" value="Search" id="submit" />
                                                </div>
                                            </td>
                                        </tr>
                                    </table> }
                                </div>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body table-responsive no-padding">
                                <table class="table table-hover">
                                    <tbody>
                                        <tr>
                                            <th>#</th>

                                            <th>@Html.DisplayNameFor(model => model.Name)</th>

                                            <th>@Html.DisplayNameFor(model => model.NumberProject)</th>
                                            <th>@Html.DisplayNameFor(model => model.Type)</th>

                                            <th>@Html.DisplayNameFor(model => model.Category)</th>

                                            <th> </th>
                                        </tr>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ClientId)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Name)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.NumberProject)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Type)
                                                </td>


                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Category)
                                                </td>

                                                <td style="white-space: nowrap; width: 1%;">
                                                    <div class="tabledit-toolbar btn-toolbar" style="text-align: left;">
                                                        @Html.ActionLink("Edit", "Edit", new { id = item.ClientId }) |
                                                        @Html.ActionLink("Details", "Details", new { id = item.ClientId }) |
                                                        @Html.ActionLink("Delete", "Delete", new { id = item.ClientId })
                                                    </div>
                                                </td>




                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>



                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </section>


