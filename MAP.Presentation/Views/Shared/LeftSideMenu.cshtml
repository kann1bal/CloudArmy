<aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="../../Assets/Back/dist/img/moez.jpg" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
                <p>Moez Baklouti</p>
                <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
            </div>
        </div>
        <!-- search form -->
        <form action="#" method="get" class="sidebar-form">
            <div class="input-group">
                <input type="text" name="q" class="form-control" placeholder="Search...">
                <span class="input-group-btn">
                    <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                        <i class="fa fa-search"></i>
                    </button>
                </span>
            </div>
        </form>
        <!-- /.search form -->
        <!-- sidebar menu: : style can be found in sidebar.less -->
        <ul class="sidebar-menu" data-widget="tree">


            <li>
                <a href=@Url.Action("Client","Back")>
                    <i class="fa fa-users"></i><span> Clients</span>

                </a>
            </li>
            <li class="treeview">
                <a href="#">
                    <i class="fa fa-briefcase"></i>
                    <span>Projects</span>
                    <span class="pull-right-container">

                    </span>
                </a>
                <ul class="treeview-menu">
                    <li><a href="@Url.Action("Projet","Back")"><span>ALL Projects</span></a></li>

                </ul>
            </li>
            <li><a href=@Url.Action("Index","POPEmail")><i class="fa fa-book"></i> <span>Email</span></a></li>

            <li class="treeview">

                <a href="#">
                    <i class="fa fa-edit"></i> <span>Resources Management</span>
                    <span class="pull-right-container">
                        <i class="fa fa-angle-left pull-right"></i>
                    </span>
                </a>
            <li>
                <a href="@Url.Action("LogOff", "Account")"></a>
            </li>
            <ul>
                <li>@Html.Partial("_LoginPartial") </li>
            </ul>




        </ul>
    </section>
    <!-- /.sidebar -->
</aside>













